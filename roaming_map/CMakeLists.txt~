cmake_minimum_required(VERSION 2.8.3)
project(roaming_map)

find_package(catkin REQUIRED COMPONENTS
  actionlib
  move_base_msgs
  roscpp
  geometry_msgs
  nav_msgs
)

catkin_package(
  CATKIN_DEPENDS actionlib move_base_msgs roscpp geometry_msgs face_dr message_runtime nav_msgs
)

include_directories(
  ${catkin_INCLUDE_DIRS}
)

add_executable(roaming_map_node src/nav_and_detect.cpp)

add_dependencies(roaming_map_node ${catkin_EXPORTED_TARGETS})


 target_link_libraries(roaming_map_node
   ${catkin_LIBRARIES}
 )
